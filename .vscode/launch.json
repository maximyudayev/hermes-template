  "version": "0.2.0",
  "configurations": [
    {
      "name": "Local Template",
      "type": "debugpy",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": "hermes-cli",
      "args": [
        "--experiment", "project=Test", "type=Template", "trial=0",
        "--config_file", "examples/test.yml",
        "--out_file", "examples/"
      ],
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "console": "integratedTerminal",
      "subProcess": true,
      "justMyCode": false
    },
  ]
}
